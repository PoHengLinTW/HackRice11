{
    "manifest_version": 3,
    "name": "OneClickMeeting",
    "version": "0.1",
    "background": {
    "service_worker": "background.js"
   },
    "action": {
        "default_icon": "images/icon.png",
        "default_popup": "mainpopup.html"
    },
    "icons": {
        "16": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
    },
    "permissions": [
        "activeTab",
        "scripting",
        "storage",
        "identity",
        "tabs",
        "identity.email",
        "background"
    ]
  // "content_security_policy": { "extension_pages":"script-src 'self' 'unsafe-eval'; object-src 'self';"}
}